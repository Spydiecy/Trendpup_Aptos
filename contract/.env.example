# TrendPup Contract Environment Configuration
# Copy this to .env and update with your values

# Deployment Configuration
NETWORK=testnet
# NETWORK=mainnet  # Uncomment for mainnet deployment

# Contract Configuration (will be set after deployment)
CONTRACT_ADDRESS=
TREASURY_ADDRESS=

# Access Control Settings
ACCESS_FEE_APT=1
ACCESS_DURATION_DAYS=30

# Frontend Integration
NEXT_PUBLIC_CONTRACT_ADDRESS=
NEXT_PUBLIC_NETWORK=testnet

# Admin Configuration (for contract management)
ADMIN_PRIVATE_KEY=
ADMIN_ADDRESS=

# Monitoring (optional)
WEBHOOK_URL=
DISCORD_WEBHOOK=
